             <process-id>
					<id>40</id>
               				<router-id>10.0.0.21</router-id>
					<network>
						<ip>10.0.0.21</ip>
						<wildcard>0.0.0.0</wildcard>
						<area>0</area>
					</network>
					<network>
						<ip>192.168.222.0</ip>
						<wildcard>0.0.0.255</wildcard>
						<area>0</area>
					</network>
               <passive-interface>
                  <interface>GigabitEthernet2</interface> 
                  <interface>GigabitEthernet3</interface> 
               </passive-interface>
                <redistribute>
						<bgp>
							<as>20</as>
						</bgp>
					</redistribute>
                </process-id>
				</ospf>
			</router-ospf>
				<bgp xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-bgp">
					<id>20</id>
					<neighbor>
						<id>172.30.20.201</id>
						<remote-as>40</remote-as>
